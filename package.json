{
  "name": "gcommands",
  "version": "9.1.0",
  "description": "Powerful and flexible framework!",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./MongoDBProvider": {
      "import": "./dist/providers/MongoDBProvider.js",
      "require": "./dist/providers/MongoDBProvider.js"
    },
    "./FirestoreProvider": {
      "import": "./dist/providers/FirestoreProvider.js",
      "require": "./dist/providers/FirestoreProvider.js"
    },
    "./KeyvProvider": {
      "import": "./dist/providers/KeyvProvider.js",
      "require": "./dist/providers/KeyvProvider.js"
    },
    "./LruCacheProvider": {
      "import": "./dist/providers/LruCacheProvider.js",
      "require": "./dist/providers/LruCacheProvider.js"
    },
    "./PrismaIOProvider": {
      "import": "./dist/providers/PrismaIOProvider.js",
      "require": "./dist/providers/PrismaIOProvider.js"
    },
    "./dist/providers/*": {
      "import": "./dist/providers/*.js",
      "require": "./dist/providers/*.js"
    }
  },
  "scripts": {
    "build": "bash scripts/build.bash",
    "lint": "npx eslint .",
    "build:tsc": "npx tsc -p . && gen-esm-wrapper dist/index.js dist/index.mjs",
    "build:fix": "node scripts/fixer.mjs",
    "build:docs": "typedoc --json docs/out-latest.json src/index.ts && node scripts/docs.mjs",
    "build:nextdocs": "typedoc --json docs/out-next.json src/index.ts && node scripts/next-docs.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Garlic-Team/GCommands.git"
  },
  "bugs": {
    "url": "https://github.com/Garlic-Team/GCommands/issues"
  },
  "homepage": "https://garlic-team.js.org/#/docs/gcommands/next/general/welcome",
  "author": "Garlic-Team",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "discord-api-types": "^0.26.1",
    "ms": "^2.1.3",
    "tslib": "^2.3.1",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@discordjs/ts-docgen": "0.3.4",
    "@google-cloud/firestore": "5.0.2",
    "@prisma/client": "3.9.1",
    "@types/keyv": "3.1.3",
    "@types/lru-cache": "5.1.1",
    "@types/node": "16.11.22",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "discord.js": "13.6.0",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "gen-esm-wrapper": "1.1.3",
    "keyv": "4.1.1",
    "lru-cache": "7.3.0",
    "mongodb": "4.3.1",
    "prettier": "2.5.1",
    "typedoc-plugin-djs-links": "1.0.4",
    "typescript": "4.5.5"
  },
  "engines": {
    "node": ">=16.6"
  }
}
